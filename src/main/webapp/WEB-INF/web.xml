<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="jee.web.blueprint" version="3.1">
	<display-name>webshoppe-commerce</display-name>
	<servlet>
		<servlet-name>CategoryCatalogServlet</servlet-name>
		<servlet-class>com.webshoppe.ecommerce.servlet.CategoryCatalogServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>CategoryCatalogServlet</servlet-name>
		<url-pattern>/category-catalog</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>